<clients-type>

   <client-type>
      <name>ie5</name>
      <userAgentPattern>.*MSIE 5.*</userAgentPattern>
      <preferredMimeType>text/html</preferredMimeType>
      <type>standard-browser</type>
      <description></description>
   </client-type>

   <client-type>
      <name>ns4</name>
      <userAgentPattern>.*Mozilla/4.*</userAgentPattern>
      <preferredMimeType>text/html</preferredMimeType>
      <type>standard-browser</type>
      <description></description>
   </client-type>

   <client-type>
      <name>Mozilla</name>
      <userAgentPattern>.*Mozilla/5.*</userAgentPattern>
      <preferredMimeType>text/xhtml</preferredMimeType>
      <type>standard-browser</type>
      <description></description>
   </client-type>

   <client-type>
      <name>Opera7</name>
      <userAgentPattern>.*Opera/7.*</userAgentPattern>
      <preferredMimeType>text/xhtml</preferredMimeType>
      <type>standard-browser</type>
      <description></description>
   </client-type>

</clients-type>
